{
  "name": "doculet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "prod": "npm run build && serve -s dist",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service s3-deploy",
    "utest": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@firebase/app": "^0.3.3",
    "@firebase/firestore": "^0.7.1",
    "@types/highlight.js": "^9.12.3",
    "@types/requirejs": "^2.1.31",
    "asciidoctor.js": "^1.5.7-beta.1",
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "brace": "^0.11.1",
    "firebase": "^5.2.0",
    "gist-client": "^1.0.7",
    "highlight.js": "^9.12.0",
    "marked-ts": "^1.0.0-beta.4",
    "requirejs": "^2.3.5",
    "typescript-debounce-decorator": "0.0.17",
    "vue": "^2.5.16",
    "vue-analytics": "^5.16.0",
    "vue-awesome": "^3.0.3",
    "vue-class-component": "^6.0.0",
    "vue-headful": "^2.0.1",
    "vue-highlightjs": "^1.3.3",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-slideout": "^1.7.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^22.0.1",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.0.4",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "dotenv": "^6.0.0",
    "node-sass": "^4.9.0",
    "prerender-spa-plugin": "^3.2.1",
    "sass-loader": "^7.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^22.4.6",
    "vue-cli-plugin-prerender-spa": "^1.0.2",
    "vue-cli-plugin-s3-deploy": "^2.1.1",
    "vue-template-compiler": "^2.5.16",
    "vue2-ace-editor": "0.0.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
  }
}
